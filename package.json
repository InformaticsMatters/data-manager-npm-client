{
  "version": "0.0.0",
  "author": "Oliver Dudgeon",
  "name": "@squonk/data-manager-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/InformaticsMatters/data-manager-npm-client"
  },
  "description": "TypeScript client for the Squonk Data Manager API. Generated from the Open API spec with orval. Provides React Hooks to fetch data with react-query using Axios under-the-hood.",
  "license": "MIT",
  "type": "module",
  "module": "./index.js",
  "main": "./index.cjs",
  "types": "./index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "orval --config ./orval.config.cjs && tsup && node setup-entrypoints.js",
    "orval": "orval --config ./orval.config.cjs"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "axios": "^0.24.0",
    "eslint": "^8.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "orval": "^6.2.3",
    "prettier": "2.4.1",
    "react-query": "^3.29.0",
    "tslib": "^2.3.1",
    "tsup": "^5.5.0",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "axios": "^0.24.0",
    "react-query": "^3.29.0"
  }
}
