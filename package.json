{
  "version": "0.0.0",
  "author": "Oliver Dudgeon",
  "name": "@squonk/data-manager-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/InformaticsMatters/data-manager-npm-client"
  },
  "description": "TypeScript client for the Squonk Data Manager API. Generated from the Open API spec with orval. Provides React Hooks to fetch data with react-query using Axios under-the-hood.",
  "license": "MIT",
  "type": "module",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "node setup.js && orval --config ./orval.config.cjs && tsup",
    "watch": "tsup --watch",
    "orval": "orval --config ./orval.config.cjs"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "eslint": "^7.29.0",
    "eslint-plugin-prettier": "^3.4.0",
    "js-yaml": "^4.1.0",
    "orval": "^5.4.11",
    "tslib": "^2.3.0",
    "tsup": "^4.12.0"
  },
  "peerDependencies": {
    "axios": "^0.21.1",
    "react-query": "^3.18.1"
  }
}
